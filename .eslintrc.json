{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "indent": ["error", 4],
        "no-tabs": [0],
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-indent": ["error", 4],
        "semi": ["error", "never"],
        "react/prop-types": [0],
        "react/forbid-prop-types": [0]
    },
    "globals": { "document": true, "window": true }
}
